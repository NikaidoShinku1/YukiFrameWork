            </uie:Toolbar>
            <ui:VisualElement name="RightContent" style="flex-grow: 1;">
                <YukiFrameWork.Missions.BackGroundView name="BackGroundView" focusable="true" style="height: auto; width: auto; flex-grow: 1;" />
            </ui:VisualElement>
        </ui:VisualElement>
    </YukiFrameWork.Missions.MissionTreeView>
