        </ui:VisualElement>
        <ui:VisualElement name="RightView" style="flex-shrink: 1; flex-grow: 0.4; width: auto; height: auto;">
            <uie:Toolbar tooltip="节点属性" style="flex-direction: row;">
                <uie:ObjectField name="MissionTreeField" allow-scene-objects="true" type="YukiFrameWork.Missions.MissionTreeSO, MissionTree" style="width: 248px; padding-left: 0;" />
